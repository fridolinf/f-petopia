{
    "env":{
      "browser": true, 
      "es6": true
    },
    "extends": ["react-app", "prettier"],
    "parserOptions":{"ecmaVersion": 2019},
    "plugins": ["react", "prettier"],
    "rules": {
      "no-console": "warn",
      "prettier/prettier":[
        "error",
        {
          "printWidth": 80,
          "trailingComma": "es5",
          "semi": true,
          "jsxSingleQuote": true,
          "singleQuote": true,
          "useTabs": true,
          "tabWidth": 2,
          "bracketSpacing": true,
          "endOfLine":"auto",
          "tabSize": 2
        }
      ]
    }
  }